<script defer src="/javascript/fileUpload.js"></script>

<%- include('../partials/navbar') %> 
<br>
<br>
<br> 
<form action="/upload" method="post">
    <div>
        <label>File</label>
        <input type="file" name="cover" class="filepond" >
      </div>
      <br>
      <div>
        <label for="Reciever"> Reciever
          <select name="Reciever" id="Reciever">
            <% publickey.forEach(key => { %>
              <option label="<%= key.email%>" value="<%= key.email %>">
            <%})  %> 
          </select>
        </label>
      </div>
      <br>
      <button type="submit">Create</button>
</form>